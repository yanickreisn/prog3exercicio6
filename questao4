# Tupla com os nomes dos alunos e suas respectivas notas
alunos_notas = ('Ana', 8.5, 'Carlos', 7.0, 'Beatriz', 9.2, 'Daniel', 6.8, 'Eduarda', 8.0)

# Exibindo apenas os nomes dos alunos
print("Nomes dos alunos:")
for i in range(0, len(alunos_notas), 2):  # Pega os nomes (índices pares)
    print(alunos_notas[i])

# Exibindo apenas as notas dos alunos
print("\nNotas dos alunos:")
for i in range(1, len(alunos_notas), 2):  # Pega as notas (índices ímpares)
    print(alunos_notas[i])
